<?xml version="1.0" encoding="utf-8"?>
<Configuration>
  <Appenders>
    <Console name="CONSOLE" target="SYSTEM_OUT">
      <PatternLayout pattern="%d{yyyy-MM-dd HH:mm:ss.SSS} %-5level [%t] %logger{36} - %msg%n"/>
    </Console>
  </Appenders>
  <Loggers>
    <Root level="{level}">
      <AppenderRef ref="CONSOLE"/>
    </Root>
  </Loggers>
</Configuration>
